    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/jest": "^24.0.20",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.9",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "rxjs": "^6.5.3",
    "tsdx": "^0.10.5",
